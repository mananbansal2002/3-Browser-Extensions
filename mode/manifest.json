{
    "manifest_version" : 2,
    "name" : "Dark Mode",
    "version" : "1.0",
    "description" : "Dark Mode",
    
    "browser_action" : {
        "default_popup" : "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" ,

    "permissions": ["tabs","https://*/*","http://*/*"]

}